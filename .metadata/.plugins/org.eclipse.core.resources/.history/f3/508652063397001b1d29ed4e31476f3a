import java.util.List;

public class Informacoes  {
	protected String nome;
	protected int idade;
	
	protected  List<Integer> pessoa; 
	
	public Informacoes(List<Integer> pessoa) {
		super();
		this.nome = nome;
		this.idade = idade;
		this.pessoa = pessoa;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public int getIdade() {
		return idade;
	}
	public void setIdade(int idade) {
		this.idade = idade;
	}
	public void addNome(){
		
	}
	
	
}
